        <plugin.class>com.persistence.plugin1.PersistenceExamplePlugin1</plugin.class>
        <plugin.version>${project.version}</plugin.version>
        <plugin.provider>StarBlues</plugin.provider>

        <gson.version>2.8.9</gson.version>
